{
  "expo": {
    "name": "Medicate",
    "slug": "MedicateRecordatorio",
    "version": "0.8.0",
    "orientation": "portrait",
    "icon": "./src/assets/medicate.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
